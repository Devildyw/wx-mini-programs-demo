.t-img{
  position:absolute;
  height: 35rpx;
  width: 35rpx;
  top: 733rpx;
  left: 610rpx;
}
.t-img-1{
  position:absolute;
  height: 35rpx;
  width: 35rpx;
  top: 595rpx;
  left: 610rpx;
}

.t-login .t-d {
  text-align: center;
  margin: 0 0 30rpx 0 ;
  color: #999;
}

.t-login .t-t{
  font-size: 28rpx;
  background: #1776bd;
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 50rpx;
  box-shadow: 1rpx 0 8rpx #1776bd;
}

.t-login .t-f{
  margin: 80rpx 0;
  text-align: center;
  color: #666;
}

.t-login .t-f text{
  padding-left: 8rpx;
  color: #999;
}

.t-login .t-e {
  text-align: center;
}

.t-login .t-e image{
  height: 50rpx;
  width: 50rpx;
}
