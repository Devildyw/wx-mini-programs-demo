  border: 1rpx solid rgb(243, 236, 234);
  border-radius: 40rpx;
  background-color: #fffbf8;
  flex-direction: column;
  box-shadow: rgba(67, 71, 85, 0.27) 0px 0px 0.25em, rgba(90, 125, 188, 0.05) 0px 0.25em 1em;
}

.info-mark-container {
  margin-top: 10rpx;
